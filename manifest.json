{
    "manifest_version": 2,
    "name": "Shopback - Check API",
    "description": "Extens√£o para analizar a api da ShopBack, dentro dos e-commerces. - Shopback",
    "version": "0.0.1",
    "author": "Mizael Silva Lemos <Vaseraga>",
    "web_accessible_resources": ["js/inject.js"],
    "browser_action": {
   	 "default_title": "Check API",
   	 "default_popup": "index.html",
   	 "default_icon": "Pepecole.png"
    },
     "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "./js/tracks.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs"
    ]
}